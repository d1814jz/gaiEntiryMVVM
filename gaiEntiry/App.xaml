<Application x:Class="gaiEntiry.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:gaiEntiry"
             xmlns:vm="clr-namespace:gaiEntiry.Base.DependencyInjection"
             xmlns:l="clr-namespace:gaiEntiry.Command"
             xmlns:c="clr-namespace:gaiEntiry.Base.Service"
             StartupUri="Base/View/AuthorizationBaseView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator"/>
            <l:DialogResultCommand x:Key="DialogResultCommand"/>
            <c:ConverterService x:Key="Convert"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
    